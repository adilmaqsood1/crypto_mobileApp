{
  "expo": {
    "name": "Hikmah AI",
    "slug": "crypto-trading-portfolio",
    "version": "1.2.0",
    "orientation": "default",
    "scheme": "hikmah-ai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/hikmah_icon.png",
    "splash": {
      "image": "./assets/hikmah_icon.png",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.hikmahai"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.hikmahai",
      "adaptiveIcon": {
        "foregroundImage": "./assets/hikmah_icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/hikmah_icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
